{
    "name": "URL Shortener",
    "description": "Shortens URLs from places like Amazon",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "hello.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "shorten.js"
    },
    "permissions": ["activeTab"]
}